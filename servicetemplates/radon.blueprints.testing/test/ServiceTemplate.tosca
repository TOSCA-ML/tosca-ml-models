tosca_definitions_version: tosca_simple_yaml_1_3

metadata:
  targetNamespace: "radon.blueprints.testing"
topology_template:
  node_templates:
    Accuracy_0:
      type: mltosca.nodes.Evaluation.Accuracy
      metadata:
        x: "1269"
        y: "88"
        displayName: "Accuracy"
      requirements:
        - host:
            node: EvaluationPipeline_0
            relationship: con_HostedOnEvaluation_0
            capability: host
    Pipeline_0:
      type: mltosca.nodes.Preprocessing.Pipeline
      metadata:
        x: "813"
        y: "408"
        displayName: "Pipeline"
      requirements:
        - dependency:
            node: ReadCSV_0
            relationship: con_DependsOn_0
            capability: feature
        - host:
            node: Conda_0
            relationship: con_HostedOn_1
            capability: host
    Precision_0:
      type: mltosca.nodes.Evaluation.Precision
      metadata:
        x: "1567"
        y: "85"
        displayName: "Precision"
      requirements:
        - host:
            node: EvaluationPipeline_0
            relationship: con_HostedOnEvaluation_1
            capability: host
    Conda_0:
      type: mltosca.nodes.Environment.Conda
      metadata:
        x: "640"
        y: "709"
        displayName: "Conda"
    ShuffleRows_0:
      type: mltosca.nodes.Preprocessing.ShuffleRows
      metadata:
        x: "796"
        y: "99"
        displayName: "ShuffleRows"
      requirements:
        - host:
            node: Pipeline_0
            relationship: con_HostedOnPipeline_0
            capability: host
    ReadCSV_0:
      type: mltosca.nodes.Reading.ReadCSV
      metadata:
        x: "208"
        y: "451"
        displayName: "ReadCSV"
      properties:
        filepath: "/home/artjom/thesis/TOSCA-ML/project1/data/data.csv"
        delimiter: ","
        encoding: "UTF-8"
      requirements:
        - host:
            node: Conda_0
            relationship: con_HostedOn_0
            capability: host
    EvaluationPipeline_0:
      type: mltosca.nodes.Evaluation.EvaluationPipeline
      metadata:
        x: "1324"
        y: "489"
        displayName: "EvaluationPipeline"
      requirements:
        - dependency:
            node: Pipeline_0
            relationship: con_DependsOn_1
            capability: feature
        - host:
            node: Conda_0
            relationship: con_HostedOn_2
            capability: host
  relationship_templates:
    con_HostedOn_2:
      type: tosca.relationships.HostedOn
    con_DependsOn_0:
      type: tosca.relationships.DependsOn
    con_DependsOn_1:
      type: tosca.relationships.DependsOn
    con_HostedOn_0:
      type: tosca.relationships.HostedOn
    con_HostedOn_1:
      type: tosca.relationships.HostedOn
    con_HostedOnEvaluation_1:
      type: mltosca.relationships.HostedOnEvaluation
    con_HostedOnPipeline_0:
      type: mltosca.relationships.HostedOnPipeline
    con_HostedOnEvaluation_0:
      type: mltosca.relationships.HostedOnEvaluation
