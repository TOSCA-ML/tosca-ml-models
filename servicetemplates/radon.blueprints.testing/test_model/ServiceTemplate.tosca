tosca_definitions_version: tosca_simple_yaml_1_3

metadata:
  targetNamespace: "radon.blueprints.testing"
topology_template:
  node_templates:
    ExportCSV_0:
      type: mltosca.nodes.Export.ExportCSV
      metadata:
        x: "1167"
        y: "490"
        displayName: "ExportCSV"
      properties:
        export_folder: "/home/artjom/Downloads"
      requirements:
        - dependency:
            node: Pipeline_0
            relationship: con_DependsOn_2
            capability: feature
        - host:
            node: Conda_0
            relationship: con_HostedOn_2
            capability: host
    RemoveNanRows_0:
      type: mltosca.nodes.Preprocessing.RemoveNanRows
      metadata:
        x: "1045"
        y: "151"
        displayName: "RemoveNanRows"
      requirements:
        - dependency:
            node: ShuffleRows_0
            relationship: con_DependsOn_1
            capability: feature
        - host:
            node: Pipeline_0
            relationship: con_HostedOnPipeline_1
            capability: host
    Pipeline_0:
      type: mltosca.nodes.Preprocessing.Pipeline
      metadata:
        x: "848"
        y: "373"
        displayName: "Pipeline"
      requirements:
        - dependency:
            node: ReadCSV_0
            relationship: con_DependsOn_0
            capability: feature
        - host:
            node: Conda_0
            relationship: con_HostedOn_1
            capability: host
    Conda_0:
      type: mltosca.nodes.Environment.Conda
      metadata:
        x: "580"
        y: "614"
        displayName: "Conda"
    ShuffleRows_0:
      type: mltosca.nodes.Preprocessing.ShuffleRows
      metadata:
        x: "635"
        y: "151"
        displayName: "ShuffleRows"
      requirements:
        - host:
            node: Pipeline_0
            relationship: con_HostedOnPipeline_0
            capability: host
    ReadCSV_0:
      type: mltosca.nodes.Reading.ReadCSV
      metadata:
        x: "314"
        y: "379"
        displayName: "ReadCSV"
      properties:
        filepath: "/home/artjom/thesis/TOSCA-ML/project1/data/data.csv"
        delimiter: ","
        encoding: "UTF-8"
      requirements:
        - host:
            node: Conda_0
            relationship: con_HostedOn_0
            capability: host
  relationship_templates:
    con_HostedOn_2:
      type: tosca.relationships.HostedOn
    con_DependsOn_0:
      type: tosca.relationships.DependsOn
    con_DependsOn_1:
      type: tosca.relationships.DependsOn
    con_HostedOn_0:
      type: tosca.relationships.HostedOn
    con_HostedOn_1:
      type: tosca.relationships.HostedOn
    con_DependsOn_2:
      type: tosca.relationships.DependsOn
    con_HostedOnPipeline_0:
      type: mltosca.relationships.HostedOnPipeline
    con_HostedOnPipeline_1:
      type: mltosca.relationships.HostedOnPipeline
