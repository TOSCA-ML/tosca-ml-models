      type: mltosca.nodes.Reading.ReadCSV
      metadata:
        x: "1610"
        y: "601"
        displayName: "ReadCSV"
      properties:
        filepath: "/home/artjom/thesis/TOSCA-ML/project1/data/data.csv"
        delimiter: ","
        encoding: "UTF-8"
      requirements:
        - dependency:
            node: Pipeline_0
            relationship: con_DependsOn_3
            capability: feature
        - host:
            node: Conda_0
            relationship: con_HostedOn_5
            capability: host
