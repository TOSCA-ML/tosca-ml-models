      metadata:
        x: "922"
        y: "109"
        displayName: "ReplaceNanValues"
      properties:
        method: "mean"
        column: "Age"
        value: "None"
      requirements:
        - dependency:
            node: RemoveColumns_0
            relationship: con_DependsOn_2
            capability: feature
        - host:
            node: Pipeline_0
            relationship: con_HostedOn_3
            capability: host
