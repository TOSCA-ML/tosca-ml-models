tosca_definitions_version: tosca_simple_yaml_1_3

metadata:
  targetNamespace: "radon.blueprints.testing"
topology_template:
  node_templates:
    Pipeline_0:
      type: mltosca.nodes.Preprocessing.Pipeline
      metadata:
        x: "890"
        y: "600"
        displayName: "Pipeline"
      requirements:
        - dependency:
            node: ReadCSV_0
            relationship: con_DependsOn_0
            capability: feature
        - host:
            node: Conda_0
            relationship: con_HostedOn_0
            capability: host
    ReplaceNanValues_1:
      type: mltosca.nodes.Preprocessing.ReplaceNanValues
      metadata:
        x: "1046"
        y: "247"
        displayName: "ReplaceNanValues"
      properties:
        method: "mode"
        column: "Embarked"
        value: "None"
      requirements:
        - dependency:
            node: RemoveColumns_0
            relationship: con_DependsOn_4
            capability: feature
        - host:
            node: Pipeline_0
            relationship: con_HostedOn_6
            capability: host
